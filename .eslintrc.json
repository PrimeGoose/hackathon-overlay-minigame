{
  "env": {
      "commonjs": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 11
  },
  "rules": {
      "no-unused-vars": ["warn", {"vars": "all", "args": "none", "ignoreRestSiblings": false}]
  }
}
